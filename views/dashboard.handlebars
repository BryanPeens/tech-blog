<h1>Dashboard</h1>
{{#each userPosts}}
<div class="post">
    <h2>{{this.title}}</h2>
    <p>{{this.content}}</p>
    <small>Posted by {{this.user.username}} on {{format_date this.createdAt}}</small>
    <button data-id="{{this.id}}" class="edit-post">Edit</button>
    <button data-id="{{this.id}}" class="delete-post">Delete</button>
</div>
{{/each}}
<button id="new-post">New Post</button>

<script src="/js/dashboard.js"></script>
